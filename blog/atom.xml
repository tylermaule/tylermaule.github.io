---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="https://www.w3.org/2005/Atom">

	<title>Tyler's Blog</title>
	<link href="http://maumaj.github.io/blog/atom.xml" rel="self"/>
	<link href="http://maumaj.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://maumaj.github.io/blog</id>
	<author>
		<name>Tyler Maule</name>
		<email>mauletj@whitman.edu</email>
	</author>

	<div class="post_backdrop">

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://maumaj.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://maumaj.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</div>

</feed>
